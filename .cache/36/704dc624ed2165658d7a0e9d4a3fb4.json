 {\r\n\tmargin: 0 auto;\r\n\tmax-width: 1200px;\r\n}\r\n\r\n/* Components */\r\n/* Header */\r\nheader {\r\n\tbackground: url('../src/assets/bg.jpg') no-repeat;\r\n\tbackground-size: cover;\r\n\tcolor: var(--color-light);\r\n\theight: 800px;\r\n\tpadding: 0 45px;\r\n}\r\n\r\n.hero__container {\r\n\tmargin-top: 15%;\r\n\ttext-align: center;\r\n}\r\n\r\n/* nav */\r\nnav {\r\n\talign-items: center;\r\n\tdisplay: flex;\r\n\tjustify-content: space-between;\r\n\tpadding-top: 30px;\r\n}\r\n\r\nnav ul {\r\n\talign-items: center;\r\n\tdisplay: flex;\r\n}\r\n\r\nnav ul li {\r\n\tmargin-right: 25px;\r\n}\r\n\r\nnav ul li:last-child {\r\n\tmargin-right: 0;\r\n}\r\n\r\nnav ul li a {\r\n\tcolor: var(--color-light);\r\n}\r\n\r\n/* info */\r\n.info {\r\n\tmargin: 170px auto;\r\n\tmax-width: 75%;\r\n\ttext-align: center;\r\n}\r\n\r\n.info h3 {\r\n\tmargin-bottom: 30px;\r\n}\r\n/* 50/50 */\r\n.block__50-50 {\r\n\tdisplay: flex;\r\n\tmargin-bottom: 130px;\r\n}\r\n\r\n.block__50-50 img {\r\n\twidth: 50%;\r\n}\r\n\r\n.block__50-50 article {\r\n\talign-items: center;\r\n\tdisplay: flex;\r\n\tflex-direction: column;\r\n\tjustify-content: center;\r\n\tpadding: 100px;\r\n\ttext-align: center;\r\n}\r\n\r\n.block__50-50 p {\r\n\tmargin: 30px 0;\r\n}\r\n\r\n.block__50-50.inverted {\r\n\tflex-direction: row-reverse;\r\n}\r\n/* subscription */\r\n.subscription {\r\n\tbackground-color: var(--color-bg-info);\r\n\tpadding: 160px 0;\r\n\ttext-align: center;\r\n}\r\n\r\n.subscription p {\r\n\tmargin: 30px 0;\r\n}\r\n\r\n.subscription .input {\r\n\tborder: 1px solid var(--color-border-input);\r\n\tfont-family: var(--font-site);\r\n\tfont-size: 1.2rem;\r\n\tfont-weight: 300;\r\n\tmargin-right: 10px;\r\n\tpadding: 22px 32px;\r\n}\r\n/* footer */\r\nfooter {\r\n\tbackground-color: var(--color-bg-footer);\r\n\tcolor: var(--color-light);\r\n\tpadding: 120px 0;\r\n\ttext-align: center;\r\n}\r\n\r\nfooter p {\r\n\tmargin-bottom: 30px;\r\n}\r\n\r\nfooter p:last-child {\r\n\tmargin-bottom: 0;\r\n}\r\n\r\nfooter a {\r\n\tcolor: var(--color-bg-button);\r\n\ttext-decoration: underline;\r\n}\r\n\r\n/* General Components */\r\n.btn {\r\n\tbackground-color: var(--color-bg-button);\r\n\tborder: none;\r\n\tborder-radius: .4rem;\r\n\tcolor: var(--color-light);\r\n\tcursor: pointer;\r\n\tfont-family: var(--font-site);\r\n\tfont-weight: 400;\r\n\tfont-size: 1.1rem;\r\n\tpadding: 22px 32px;\r\n\ttransition: all 300ms linear;\r\n}\r\n\r\n.btn:hover {\r\n\tbackground-color: var(--color-bg-button-hover);\r\n\topacity: .7;\r\n}\r\n\r\n.btn--small {\r\n\tpadding: 10px 32px;\r\n}"},"lineCount":null}},"error":null,"hash":"f915ded7d3db732f6ce3f5f522feb132","cacheData":{"env":{}}}